{
  "expo": {
    "name": "Caf√© sans-fil",
    "slug": "cafe-sans-fil",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses the location to find cafes near you."
      },
      "bundleIdentifier": "com.cadum.cafe-sans-fil"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "logo.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "46340bc6-102f-4adf-81cc-8fb3eb582e09"
      }
    },
    "owner": "cadum"
  }
}
