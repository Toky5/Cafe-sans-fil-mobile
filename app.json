{
  "expo": {
    "name": "Café sans-fil",
    "slug": "cafe-sans-fil",
    "version": "1.1.4",
    "orientation": "portrait",
    "icon": "logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "30",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Autorisez Café sans-fil à accéder à votre position pour vous montrer les cafés à proximité !",
        "NSUserNotificationsUsageDescription": "Autorisez les notifications pour recevoir des mises à jour sur les nouveaux cafés et offres spéciales !",
        "ITSAppUsesNonExemptEncryption": false,
        "NSPhotoLibraryUsageDescription": "Autorisez l'accès à votre bibliothèque de photos pour que vous puissiez télécharger une photo de profile !"
      },
      "bundleIdentifier": "com.cadum.cafe-sans-fil",
      "appleTeamId": "XY2S94A7AL",
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "androdlogo.png",
        "backgroundColor": "#fbfaf7"
      },
      "package": "com.cadum.cafesansfil",
      "versionCode": 30,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBLPZng6Gr0UCGxWrovmdbBaVmptRQEUHs"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "logoold.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "46340bc6-102f-4adf-81cc-8fb3eb582e09"
      }
    },
    "owner": "cadum"
  }
}